{
    "manifest_version": 2,
    "name": "Novus",
    "short_name": "A New Tab UI for Chrome",
    "description": "Novus is an awesome New Tab interface that enhances bookmark management",
    "version": "1.17",
    "author": "Roy Prigat",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "novus.png",
        "default_title": "Novus New Tab"
    },
    "web_accessible_resources": [
        "fonts/"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permissions": [
        "identity",
        "storage",
        "bookmarks",
        "geolocation",
        "*://*.api.unsplash.com//*",
        "https://fonts.googleapis.com/"
    ],
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
    "oauth2": {
        "client_id": "413690262998-i050041d7fqtak7ib0qiuua773hf4c83.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/gmail.readonly"
        ]
    },
    "icons": {
        "16": "novus.png",
        "32": "novus.png",
        "48": "novus.png",
        "128": "novus.png"
    },
    "key": ""
}