{
    "manifest_version": 2,
    "name": "Novus",
    "description": "The ultimate new tab window for front end enthusiasts",
    "version": "1",
    "author": "Roy Prigat",
    "browser_action": {
        "default_icon": "novus.png",
        "default_title": "Novus New Tab"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permissions": ["activeTab"]
}